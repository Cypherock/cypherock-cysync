{
  "name": "@cypherock/cysync-ui",
  "version": "0.0.0",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/cjs/index.d.ts",
  "license": "AGPL-3.0",
  "private": true,
  "scripts": {
    "lint": "eslint \"src/**/*.{ts,tsx}\" --fix",
    "lint:check": "eslint \"src/**/*.{ts,tsx}\"",
    "pretty": "prettier --write \"src/**/*.ts?(x)\" \"tests/**/*.ts?(x)\"",
    "pretty:check": "prettier --check \"src/**/*.ts?(x)\" \"tests/**/*.ts?(x)\"",
    "build": "rimraf dist && pnpm build:esm && pnpm build:cjs",
    "build:esm": "tsc -p tsconfig.json && copyfiles -u 1 src/**/*.{png,jpg,svg,jpeg} dist/esm",
    "build:cjs": "tsc -p tsconfig_cjs.json && copyfiles -u 1 src/**/*.{png,jpg,svg,jpeg} dist/cjs",
    "watch:build": "tsc --watch -p tsconfig.json",
    "test": "jest",
    "pre-commit": "lint-staged"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.22.5",
    "@cypherock/eslint-config": "workspace:^",
    "@cypherock/jest-config": "workspace:^",
    "@cypherock/prettier-config": "workspace:^",
    "@cypherock/tsconfig": "workspace:^",
    "@jest/globals": "^29.5.0",
    "@stryker-mutator/core": "^6.4.2",
    "@stryker-mutator/jest-runner": "^6.4.2",
    "@stryker-mutator/typescript-checker": "^6.4.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.2",
    "@types/node": "18.15.11",
    "@types/react": "^18.2.12",
    "@types/react-dom": "^18.2.5",
    "@types/styled-components": "^5.1.26",
    "copyfiles": "^2.4.1",
    "eslint": "^8.42.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "ts-jest": "^29.1.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-tsparticles": "^2.10.1",
    "styled-components": "6.0.0-rc.1",
    "tsparticles": "^2.10.1"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --ext ts,tsx --quiet --fix --",
      "prettier --write"
    ],
    "*.{js,jsx,md,mdx,mjs,yml,yaml,css,json}": [
      "prettier --write"
    ]
  }
}
