{
  "name": "@cypherock/cysync-ui",
  "version": "0.0.2",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "license": "AGPL-3.0",
  "private": true,
  "scripts": {
    "lint": "eslint \"src/**/*.{ts,tsx}\" --fix",
    "lint:check": "eslint \"src/**/*.{ts,tsx}\"",
    "pretty": "prettier --write \"src/**/*.ts?(x)\" \"tests/**/*.ts?(x)\"",
    "pretty:check": "prettier --check \"src/**/*.ts?(x)\" \"tests/**/*.ts?(x)\"",
    "build": "pnpm clean && pnpm build:esm && pnpm build:cjs",
    "build:dirty": "tsc -p tsconfig.json",
    "build:esm": "tsc -p tsconfig.json && copyfiles -u 1 src/**/*.{png,jpg,svg,jpeg,gif,webm} dist/esm",
    "build:cjs": "tsc -p tsconfig_cjs.json && copyfiles -u 1 src/**/*.{png,jpg,svg,jpeg,gif,webm} dist/cjs",
    "build:icons": "svgr icons --out-dir ./src/assets/icons/generated --no-svgo",
    "prebuild": "pnpm build:icons",
    "clean": "rimraf dist",
    "test": "jest",
    "pre-commit": "lint-staged",
    "storybook": "storybook dev -p 6006",
    "build:storybook": "storybook build"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.22.5",
    "@cypherock/eslint-config": "workspace:^",
    "@cypherock/jest-config": "workspace:^",
    "@cypherock/prettier-config": "workspace:^",
    "@cypherock/tsconfig": "workspace:^",
    "@jest/globals": "^29.5.0",
    "@storybook/addon-essentials": "7.2.0",
    "@storybook/addon-interactions": "7.2.0",
    "@storybook/addon-links": "7.2.0",
    "@storybook/addon-onboarding": "1.0.8",
    "@storybook/addon-styling": "^1.3.5",
    "@storybook/blocks": "7.2.0",
    "@storybook/jest": "^0.1.0",
    "@storybook/react": "7.2.0",
    "@storybook/react-vite": "7.2.0",
    "@storybook/testing-library": "0.2.0",
    "@stryker-mutator/core": "^7.0.2",
    "@stryker-mutator/jest-runner": "^7.0.2",
    "@stryker-mutator/typescript-checker": "^7.0.2",
    "@svgr/cli": "^8.0.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.2",
    "@types/node": "18.15.11",
    "@types/react": "^18.2.13",
    "@types/react-dom": "^18.2.6",
    "@types/styled-components": "^5.1.26",
    "copyfiles": "^2.4.1",
    "eslint": "^8.43.0",
    "eslint-plugin-storybook": "^0.6.13",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "storybook": "7.2.0",
    "ts-jest": "^29.1.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@storybook/addon-docs": "^7.2.1",
    "@storybook/client-api": "^7.2.1",
    "@types/chart.js": "^2.9.37",
    "@types/lodash": "^4.14.195",
    "@types/react-virtualized": "^9.21.22",
    "chart.js": "^4.3.2",
    "date-fns": "^2.30.0",
    "focus-trap-react": "^10.2.3",
    "marked-react": "^2.0.0",
    "rc-tooltip": "^6.1.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-fast-marquee": "^1.6.2",
    "react-json-view": "^1.21.3",
    "react-multi-date-picker": "^4.1.2",
    "react-tsparticles": "^2.10.1",
    "react-virtualized": "^9.22.5",
    "styled-components": "6.0.0-rc.1",
    "tsparticles": "^2.10.1"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --ext ts,tsx --quiet --fix --",
      "prettier --write"
    ],
    "*.{js,jsx,md,mdx,mjs,yml,yaml,css,json}": [
      "prettier --write"
    ]
  }
}
