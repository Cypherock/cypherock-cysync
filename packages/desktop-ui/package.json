{
  "name": "@cypherock/cysync-desktop-ui",
  "version": "0.0.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "private": true,
  "scripts": {
    "lint": "eslint src/* --fix",
    "lint:check": "eslint --ext .ts,tsx,js,jsx,js src/",
    "pretty": "prettier --write 'src/**/*.ts?(x)' 'tests/**/*.ts?(x)'",
    "pretty:check": "prettier --check 'src/**/*.ts?(x)' 'tests/**/*.ts?(x)'",
    "build": "rimraf dist && tsc -p tsconfig.json",
    "test": "jest",
    "pre-commit": "lint-staged"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.21.4",
    "@jest/globals": "^29.4.1",
    "@stryker-mutator/core": "^6.4.1",
    "@stryker-mutator/jest-runner": "^6.4.1",
    "@stryker-mutator/typescript-checker": "^6.4.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@types/jest": "^29.5.0",
    "@types/node": "18.11.18",
    "@types/react": "^18.0.33",
    "@types/react-dom": "^18.0.11",
    "eslint": "^7.32.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "lint-staged": "^13.2.0",
    "prettier": "^2.8.7",
    "rimraf": "^4.1.2",
    "ts-jest": "^29.0.5",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --ext ts,tsx --quiet --fix --",
      "prettier --write"
    ],
    "*.{js,jsx,md,mdx,mjs,yml,yaml,css,json}": [
      "prettier --write"
    ]
  }
}
