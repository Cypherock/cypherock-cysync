    "react-redux": "^8.1.1",
    "react-router-dom": "^6.13.0",
    "react-virtualized": "^9.22.5",
    "rxjs": "^7.8.1",
    "semver": "^7.5.3",
    "styled-components": "^6.0.0-rc.1",
    "uuid": "^9.0.0",
    "zod": "^3.21.4"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --ext ts,tsx --quiet --fix --",
      "prettier --write"
    ],
    "*.{js,jsx,md,mdx,mjs,yml,yaml,css,json}": [
      "prettier --write"
    ]
  }
}
