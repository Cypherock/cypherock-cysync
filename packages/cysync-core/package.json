{
  "name": "@cypherock/cysync-core",
  "version": "0.0.0",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/cjs/index.d.ts",
  "license": "AGPL-3.0",
  "private": true,
  "scripts": {
    "lint": "eslint \"src/**/*.{ts,tsx}\" --fix",
    "lint:check": "eslint \"src/**/*.{ts,tsx}\"",
    "pretty": "prettier --write \"src/**/*.ts?(x)\" \"tests/**/*.ts?(x)\"",
    "pretty:check": "prettier --check \"src/**/*.ts?(x)\" \"tests/**/*.ts?(x)\"",
    "build": "rimraf dist && pnpm build:esm && pnpm build:cjs",
    "build:esm": "tsc -p tsconfig.json && tsc-alias -p tsconfig.json && copyfiles -u 1 src/**/*.{png,jpg,svg,jpeg} dist/esm",
    "build:cjs": "tsc -p tsconfig_cjs.json && tsc-alias -p tsconfig_cjs.json && copyfiles -u 1 src/**/*.{png,jpg,svg,jpeg} dist/cjs",
    "watch:build": "concurrently \"tsc --watch -p tsconfig.json\" \"tsc-alias --watch\"",
    "test": "jest",
    "pre-commit": "lint-staged"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.21.5",
    "@cypherock/eslint-config": "workspace:^",
    "@cypherock/jest-config": "workspace:^",
    "@cypherock/prettier-config": "workspace:^",
    "@cypherock/tsconfig": "workspace:^",
    "@jest/globals": "^29.4.1",
    "@stryker-mutator/core": "^6.4.1",
    "@stryker-mutator/jest-runner": "^6.4.1",
    "@stryker-mutator/typescript-checker": "^6.4.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.1",
    "@types/node": "18.15.11",
    "@types/prop-types": "^15.7.5",
    "@types/react": "^18.0.33",
    "@types/react-dom": "^18.0.11",
    "copyfiles": "^2.4.1",
    "eslint": "^8.38.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "identity-obj-proxy": "^3.0.0",
    "immer": "9.0.21",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "ts-jest": "^29.1.0",
    "tsc-alias": "^1.8.6",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@cypherock/coins": "workspace:^",
    "@cypherock/cysync-interfaces": "workspace:^",
    "@cypherock/cysync-ui": "workspace:^",
    "@cypherock/cysync-utils": "workspace:^",
    "@cypherock/db-interfaces": "workspace:^",
    "@cypherock/sdk-app-manager": "^0.0.6",
    "@cypherock/sdk-core": "^0.0.5",
    "@cypherock/sdk-interfaces": "^0.0.4",
    "@cypherock/sdk-utils": "^0.0.4",
    "@reduxjs/toolkit": "^1.9.5",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.11.2",
    "styled-components": "^6.0.0-rc.1"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --ext ts,tsx --quiet --fix --",
      "prettier --write"
    ],
    "*.{js,jsx,md,mdx,mjs,yml,yaml,css,json}": [
      "prettier --write"
    ]
  }
}
