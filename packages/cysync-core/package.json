    "rimraf": "^5.0.1",
    "safe-buffer": "^5.2.1",
    "ts-jest": "^29.1.0",
    "tsc-alias": "^1.8.6",
    "typescript": "^4.9.5",
    "web3": "^4.1.2"
  },
  "dependencies": {
    "@aws-crypto/sha256-browser": "^4.0.0",
    "@cypherock/coin-support": "workspace:^",
    "@cypherock/coin-support-btc": "workspace:^",
    "@cypherock/coin-support-evm": "workspace:^",
    "@cypherock/coin-support-interfaces": "workspace:^",
