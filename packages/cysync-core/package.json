{
  "name": "@cypherock/cysync-core",
  "version": "0.0.2",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "license": "AGPL-3.0",
  "private": true,
  "scripts": {
    "lint": "eslint \"src/**/*.{ts,tsx}\" --fix",
    "lint:check": "eslint \"src/**/*.{ts,tsx}\"",
    "pretty": "prettier --write \"src/**/*.ts?(x)\" \"tests/**/*.ts?(x)\"",
    "pretty:check": "prettier --check \"src/**/*.ts?(x)\" \"tests/**/*.ts?(x)\"",
    "build": "rimraf dist && pnpm build:esm",
    "build:dirty": "pnpm build:esm",
    "build:esm": "tsc -p tsconfig.json && tsc-alias -p tsconfig.json --outDir=./dist/esm && copyfiles -u 1 src/**/*.{png,jpg,svg,jpeg,js,gif,webm} src/**/**/*.{png,jpg,svg,jpeg,js,gif,webm} dist/esm",
    "build:cjs": "tsc -p tsconfig_cjs.json && tsc-alias -p tsconfig_cjs.json --outDir=./dist/cjs && copyfiles -u 1 src/**/*.{png,jpg,svg,jpeg,js,gif,webm} src/**/**/*.{png,jpg,svg,jpeg,js,gif,webm} dist/cjs",
    "build:dependencies": "bash ./scripts/prebuild.sh",
    "test": "jest",
    "pre-commit": "lint-staged",
    "prebuild": "pnpm build:dependencies"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.22.5",
    "@cypherock/eslint-config": "workspace:^",
    "@cypherock/jest-config": "workspace:^",
    "@cypherock/prettier-config": "workspace:^",
    "@cypherock/tsconfig": "workspace:^",
    "@jest/globals": "^29.5.0",
    "@stryker-mutator/core": "^7.0.2",
    "@stryker-mutator/jest-runner": "^7.0.2",
    "@stryker-mutator/typescript-checker": "^7.0.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.2",
    "@types/lodash": "^4.14.195",
    "@types/node": "18.15.11",
    "@types/prop-types": "^15.7.5",
    "@types/react": "^18.2.13",
    "@types/react-dom": "^18.2.6",
    "@types/react-virtualized": "^9.21.22",
    "@types/semver": "^7.5.0",
    "bitcoinjs-lib": "^6.1.3",
    "browserify": "^17.0.0",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.43.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "ethers": "^6.7.0",
    "identity-obj-proxy": "^3.0.0",
